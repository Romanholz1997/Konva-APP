{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
, "../Canvas.tsx", "../Circle.tsx", "../DraggableList.tsx", "../Main.tsx", "../MouseMove.tsx", "../MultiSelect.tsx", "../Rectangle.tsx", "../Ruler.tsx", "../RulerCanvas.tsx", "../Sidebar.tsx", "../Star.tsx"  ]
}
